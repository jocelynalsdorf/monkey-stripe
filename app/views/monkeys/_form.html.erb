<%= render "layouts/errors", :object => @monkey %>

<%= form_for @monkey  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <%= f.label :gender %>
  <%= f.text_field :gender %>
  <%= f.label :story %>
  <%= f.text_area :story %>
  <%= f.submit %>
<% end %>